{
  "name": "project-module-updater",
  "version": "1.4.0",
  "description": "Automatically update a module in several projects",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "clean": "shx rm -rf dist/*",
    "build": "tsc",
    "watch": "tsc --watch",
    "prepack": "npm run clean && npm run build"
  },
  "bin": {
    "pmu": "bin/run",
    "project-module-updater": "bin/run"
  },
  "files": [
    "bin",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "module",
    "update",
    "projects"
  ],
  "author": "amcereijo <amcereijo@gmail.com>",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/amcereijo/project-module-updater",
    "type": "git"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/commander": "^2.12.2",
    "@types/debug": "^4.1.5",
    "@types/node": "^14.0.13",
    "shx": "^0.3.2",
    "tslint": "^6.1.2",
    "tslint-config-airbnb-base": "^0.3.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "commander": "^5.1.0",
    "debug": "^4.1.1",
    "kleur": "^3.0.3",
    "prompts": "^2.3.2"
  }
}
