{
  "name": "project-module-updater",
  "version": "1.6.16",
  "description": "Automatically update a module in several projects",
  "main": "dist/index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha -r ts-node/register src/**/*spec.ts",
    "lint": "eslint . --ext .ts",
    "clean": "shx rm -rf dist/*",
    "build": "tsc",
    "watch": "tsc --watch",
    "prepack": "npm run clean && npm run build"
  },
  "bin": {
    "pmu": "bin/run",
    "project-module-updater": "bin/run"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "module",
    "update",
    "projects"
  ],
  "author": "amcereijo <amcereijo@gmail.com>",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/amcereijo/project-module-updater",
    "type": "git"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/chai": "^4.2.12",
    "@types/commander": "^2.12.2",
    "@types/debug": "^4.1.5",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "chai": "^4.2.0",
    "eslint": "^7.3.1",
    "eslint-plugin-import": "^2.21.2",
    "mocha": "^8.1.3",
    "shx": "^0.3.2",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "commander": "^5.1.0",
    "debug": "^4.1.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "kleur": "^3.0.3",
    "prompts": "^2.3.2"
  }
}
